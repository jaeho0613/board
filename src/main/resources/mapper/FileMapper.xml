<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.board.repository.FileMapper">

  <select id="selectFileById" resultType="File" parameterType="int">

    select *
    from t_file
    where file_seq = #{seq}

  </select>

</mapper>